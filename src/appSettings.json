{
  "ConnectionStrings": {
    "ArchiveDb": "Data Source=archive.db"
  },
  "Archive": {
    "MaxConcurrentJobs": 1,
    "ArchiveScheduleEnabled": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Verbose",
      "Microsoft": "Information",
      "System": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs\\archive-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 14,
          "shared": true
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  },
  "Quartz": {
    "UseInMemoryStore": false,
    "UseSQLite": true,
    "ConnectionString": "Data Source=archive.db",
    "MaxConcurrency": 10,
    "MisfireThreshold": "00:01:00"
  }
}
